<h3><a class="btn btn-secondary" href="/hotdog"><i class="fa fa-plus"></i> Create new</a>   Hotdog list</h3>
<table class="table table-striped">
<thead>
    <tr>
        <th>Name</th>
        <th>ID</th>
        <th></th>
    </tr>
</thead>
<tbody>
           {{#each list}}
    <tr>
    <td>{{this.Name}}</td>
    <td>{{this.ID}}</td>
    <td>
        <a href="/hotdog/{{this._id}}"><i style="color:aliceblue" class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
        <a href="/hotdog/delete/{{this._id}}" onclick="return confirm ('Are you sure to deiete this record?');"><i style="color:aliceblue" class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
    </td>
    </tr> 
           {{/each}}
</tbody>
</table>